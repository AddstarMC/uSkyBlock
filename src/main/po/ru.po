msgid "Biome: {0}"
msgstr "Биом: {0}"

#: us/talabrek/ultimateskyblock/menu/SkyBlockMenu.java:273
msgid "§2§lThis is your current biome."
msgstr "§2§lЭто ваш текущий биом."

#: us/talabrek/ultimateskyblock/menu/SkyBlockMenu.java:275
msgid "§e§lClick to change to this biome."
msgstr "§e§lНажмите, чтобы изменить биом."

#: us/talabrek/ultimateskyblock/menu/SkyBlockMenu.java:279
